  "bugs": {
    "url": "https://github.com/RolandJansen/intermix-plugin-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandJansen/intermix-plugin-template.git"
  },
  "keywords": [
    "intermix",
    "audio",
    "plugin"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && rollup --config",
    "start": "npm run clean && cross-env TARGET='debug' rollup --config --watch",
    "test": "jest --coverage",
    "watch": "jest --watch"
  },
  "private": true,
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "intermix": "file:intermix-0.6.0.tgz",
    "redux": "4.x"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@wessberg/rollup-plugin-ts": "^1.3.3",
    "cross-env": "^7.0.2",
    "jest": "^26.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-live-server": "^2.0.0",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  }
