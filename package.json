{
  "name": "intermix-plugin-template",
  "description": "A template for intermix plugin developers.",
  "author": "Roland Jansen",
  "version": "1.0.0",
  "license": "MIT",
  "entry": "src/index.ts",
  "homepage": "https://github.com/RolandJansen/intermix-plugin-template#readme",
  "bugs": {
    "url": "https://github.com/RolandJansen/intermix-plugin-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandJansen/intermix-plugin-template.git"
  },
  "keywords": [
    "intermix",
    "audio",
    "plugin"
  ],

  "scripts": {
    "build": "rollup -c",
    "test": "jest --coverage",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "watch-test": "jest --watch"
  },
  "private": true,
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost/"
  },
  "peerDependencies": {
    "redux": "4.x",
    "intermix": "file:intermix-0.6.0.tgz"
  },
  "devDependencies": {
    "@surma/rollup-plugin-off-main-thread": "^1.4.1",
    "@types/jest": "^26.0.9",
    "@wessberg/rollup-plugin-ts": "^1.3.3",
    "jest": "^26.3.0",
    "rollup": "^2.26.5",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7"
  }
}
