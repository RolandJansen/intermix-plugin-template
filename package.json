{
  "name": "intermix-plugin-template",
  "description": "A template for intermix plugin developers.",
  "author": "Roland Jansen",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/RolandJansen/intermix-plugin-template#readme",
  "bugs": {
    "url": "https://github.com/RolandJansen/intermix-plugin-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandJansen/intermix-plugin-template.git"
  },
  "keywords": [
    "intermix",
    "audio",
    "plugin"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "test": "jest --coverage"
  },
  "private": true,
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost/"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "jest": "^26.3.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  }
}
